{
	"schemaVersion": 1,
	"id": "clothesbin",
	"version": "${version}",
	"name": "Clothesbin",
	"description": "A server-side cosmetic armor management system.",
	"authors": [
		"haykam821"
	],
	"contact": {
		"homepage": "https://github.com/haykam821/Clothesbin#readme",
		"issues": "https://github.com/haykam821/Clothesbin/issues"
	},
	"entrypoints": {
